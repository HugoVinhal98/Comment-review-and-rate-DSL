		List<<#=this.Model.Subject[0].Name#>> listSandwich = new ArrayList<<#=this.Model.Subject[0].Name#>>();
		listSandwich.add(sandwich1);
		listSandwich.add(sandwich2);

		return new GorgeousSandwich(listSandwich);

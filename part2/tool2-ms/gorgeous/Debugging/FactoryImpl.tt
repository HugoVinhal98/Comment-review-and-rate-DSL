		listReviewsComment2.add(comment2);

		<#=this.Model.Subject[0].Name#> sandwich1 = new <#=this.Model.Subject[0].Name#>("X-Tudo", "Salgado", 7, "imagem.png", "tem tudo",  listReviewsComment1);
